{
  "name": "road",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev:front": "webpack serve --env port=3000 mode=development apiUrl=http://localhost:3005/api settlementApiUrl=http://localhost:80/api",
    "start:dev:vite:front": "vite",
    "start:dev:project": "concurrently \"npm run start:dev:front\" \"npm run mock-server:start\"",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "lint:ts:check": "eslint '**/*.{ts,tsx}'",
    "lint:ts:fix": "eslint '**/*.{ts,tsx}' --fix",
    "lint:scss:check": "npx stylelint '**/*.scss'",
    "lint:scss:fix": "npx stylelint '**/*.scss' --fix",
    "test:unit": "jest --silent --config './config/jest/jest.config.ts'",
    "test:ui": "npx loki test",
    "test:ui:approve": "npx loki approve",
    "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static --chromeFlags='--headless --disable-gpu --hide-scrollbars --no-sandbox'",
    "test:ui:report": "node scripts/generate-visual-screenshot-test-report.js && reg-cli --from .loki/report.json --report .loki/report.html && node scripts/openBrowserWithReport.js",
    "test:e2e": "cypress open",
    "test:all": "npm run build:prod && npm run lint:ts:check && npm run lint:scss:check && npm run test:unit && npm run documentation:storybook:build && npm run test:ui:ci && echo All tests passed successfully",
    "analyze:prod": "webpack --env mode=production analyze=1",
    "analyze:dev": "webpack --env mode=development analyze=1",
    "documentation:storybook:dev": "start-storybook -p 6006 -c ./config/storybook",
    "documentation:storybook:build": "build-storybook -c ./config/storybook",
    "mock-server:start": "node ./json-server/index.js",
    "prepare": "husky install",
    "generate:fsd": "node ./scripts/createSlice/index.js",
    "generate:dependency-graph": "npx depcruise src --include-only '^src' --output-type dot | dot -T svg > dependency-graph.svg",
    "check:code": "npx depcruise src",
    "postinstall": "node ./scripts/clearNodeModulesCache.js",
    "remove-feature": "ts-node ./scripts/remove-feature.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FURY-PERSON/road.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FURY-PERSON/road/issues"
  },
  "homepage": "https://github.com/FURY-PERSON/road#readme",
